    <DataSet Name="adresse1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>DECLARE @choix_de_l_adresse VARCHAR(38) 

SELECT  TableV_EtablissementsListePixi.Siren, CASE WHEN TableV_EtablissementsListePixi.Adresse1_Eco IS NULL 
                                                                                                                                         THEN '' WHEN TableV_EtablissementsListePixi.Adresse1_Post IS NULL 
                                                                                                                                         THEN '' WHEN (TableV_EtablissementsListePixi.Adresse1_Eco IS NOT NULL OR
                                                                                                                                         TableV_EtablissementsListePixi.Adresse1_Post IS NOT NULL) AND 
                                                                                                                                         @choix_de_l_adresse = 'Adresse Postale' THEN Adresse1_Post ELSE Adresse1_Eco END AS adresse1
                                                                                                                FROM            TableV_EtablissementsListePixi</CommandText>
      </Query>
      <Fields>
        <Field Name="Siren">
          <DataField>Siren</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="adresse1">
          <DataField>adresse1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="sigleenseigne">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>    SELECT  
		Siren,
        CASE	WHEN Code_Juridique LIKE 'M%' AND Enseigne IS NOT NULL AND Enseigne &lt;&gt; '' THEN '      / ' +  UPPER(LEFT(Enseigne,1))+LOWER(SUBSTRING(Enseigne,2,LEN(Enseigne)))
				WHEN (TableV_EtablissementsListePixi.Sigle IS NULL OR TableV_EtablissementsListePixi.Sigle ='')
					AND (TableV_EtablissementsListePixi.Enseigne IS NULL OR TableV_EtablissementsListePixi.Enseigne ='') THEN ''
                ELSE UPPER(LEFT(Sigle,1))+LOWER(SUBSTRING(Sigle,2,LEN(Sigle))) + ' / ' + UPPER(LEFT(Enseigne,1))+LOWER(SUBSTRING(Enseigne,2,LEN(Enseigne)))
            END as SigleEnseigne1
            FROM    TableV_EtablissementsListePixi</CommandText>
      </Query>
      <Fields>
        <Field Name="Siren">
          <DataField>Siren</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SigleEnseigne1">
          <DataField>SigleEnseigne1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="categorie">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>	SELECT 
		Siren,
		CASE 
			WHEN Code_Categ_Ets  IS NULL THEN ''
			WHEN Code_Categ_Ets  IS NOT NULL AND Code_Categ_Ets = '1' THEN 'Siège'
			WHEN Code_Categ_Ets  IS NOT NULL AND Code_Categ_Ets = '2' THEN 'Siège &amp; Principal'
			WHEN Code_Categ_Ets  IS NOT NULL AND Code_Categ_Ets = '3' THEN 'Principal'
			WHEN Code_Categ_Ets  IS NOT NULL AND Code_Categ_Ets = '4' THEN 'Secondaire'
			WHEN Code_Categ_Ets  IS NOT NULL AND Code_Categ_Ets = '5' THEN '1er ets en France'
			WHEN Code_Categ_Ets  IS NOT NULL AND Code_Categ_Ets = '6' THEN 'Ancien Siège'
			ELSE ''
			END AS categorie
	FROM TableV_EtablissementsListePixi</CommandText>
      </Query>
      <Fields>
        <Field Name="Siren">
          <DataField>Siren</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="categorie">
          <DataField>categorie</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="internet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>	SELECT 
		Siren,
		CASE 
			WHEN 
				(internet IS NULL OR internet ='')  THEN '{Pas de site web}'
			WHEN
				LEN(internet) &gt; 50 THEN '{site trop long}'
			ELSE
				internet
			END as internet
	FROM TableV_EtablissementsListePixi</CommandText>
      </Query>
      <Fields>
        <Field Name="Siren">
          <DataField>Siren</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="internet">
          <DataField>internet</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="siret">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>	SELECT 
		Siren,
		LEFT(Siren,3) + ' ' + substring(Siren,4,3) + ' ' + RIGHT(Siren,3) + ' ' + Nic AS Siret
	FROM TableV_EtablissementsListePixi</CommandText>
      </Query>
      <Fields>
        <Field Name="Siren">
          <DataField>Siren</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Siret">
          <DataField>Siret</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
