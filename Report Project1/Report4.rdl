    <DataSet Name="adresse2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@choix_de_l_adresse">
            <Value>=Parameters!choix_de_l_adresse.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @choix_de_l_adresse VARCHAR(38) 
SELECT        TableV_EtablissementsListePixi.Siren, CASE WHEN (Adresse2_Post IS NULL OR
                                                                                                                                         Bp_Post IS NULL) THEN '' WHEN (Adresse2_Post IS NOT NULL AND Bp_Post IS NOT NULL AND 
                                                                                                                                         Adresse2_Eco IS NOT NULL) AND Bp_Post &lt;&gt; '' AND 
                                                                                                                                         @choix_de_l_adresse = 'Adresse Postale' THEN Adresse2_Post + ' - ' + Bp_Post WHEN (Adresse2_Post IS NOT NULL
                                                                                                                                          AND Bp_Post IS NOT NULL AND Adresse2_Eco IS NOT NULL) AND 
                                                                                                                                         TableV_EtablissementsListePixi.Bp_Post = '' AND 
                                                                                                                                         @choix_de_l_adresse = 'Adresse Postale' THEN Adresse2_Post ELSE CASE WHEN Adresse2_Eco IS NOT NULL 
                                                                                                                                         THEN Adresse2_Eco ELSE '' END END AS adresse2
                                                                                                                FROM            TableV_EtablissementsListePixi</CommandText>
      </Query>
      <Fields>
        <Field Name="Siren">
          <DataField>Siren</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="adresse2">
          <DataField>adresse2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="cpville">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@choix_de_l_adresse">
            <Value>=Parameters!choix_de_l_adresse.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @choix_de_l_adresse VARCHAR(38) 
SELECT        Siren, CASE WHEN (Code_Postal_Eco IS NULL OR
                                                                                                                                         Code_Postal_Post IS NULL OR
                                                                                                                                         Libel_Commune_Eco IS NULL OR
                                                                                                                                         Libel_Commune_Post IS NULL) THEN ' ' WHEN @choix_de_l_adresse = 'Adresse Postale' AND 
                                                                                                                                         Code_Postal_Post IS NOT NULL AND Libel_Commune_Post IS NOT NULL 
                                                                                                                                         THEN Code_Postal_Post + ' ' + Libel_Commune_Post ELSE Code_Postal_Eco + '  ' + Libel_Commune_Eco END AS
                                                                                                                                          cpville
                                                                                                                FROM            TableV_EtablissementsListePixi</CommandText>
      </Query>
